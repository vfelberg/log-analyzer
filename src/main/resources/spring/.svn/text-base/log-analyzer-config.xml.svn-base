    <bean id="staging" class="com.smeet.loganalyzer.config.Environment">
        <property name="environmentName" value="Staging" />
        <property name="hostGroupConfigs">
            <list>
                <bean class="com.smeet.loganalyzer.config.HostGroupConfig">
                    <property name="groupName" value="Web" />
                    <property name="hostConfigs">
                        <list>
                            <ref bean="s_webapp_1" />
                            <ref bean="s_webapp_2" />
                        </list>
                    </property>
                    <property name="baseLogFileName" value="smeet-web.log" />
                </bean>
                <bean class="com.smeet.loganalyzer.config.HostGroupConfig">
                    <property name="groupName" value="Services" />
                    <property name="hostConfigs">
                        <list>
                            <ref bean="s_generic_1" />
                            <ref bean="s_generic_2" />
                        </list>
                    </property>
                    <property name="baseLogFileName" value="smeet-services.log" />
                </bean>
                <bean class="com.smeet.loganalyzer.config.HostGroupConfig">
                    <property name="groupName" value="Jobs" />
                    <property name="hostConfigs">
                        <list>
                            <ref bean="s_generic_3" />
                        </list>
                    </property>
                    <property name="baseLogFileName" value="smeet-jobs.log" />
                </bean>
            </list>
        </property>
        <property name="zooKeeper">
            <bean class="com.smeet.zookeeper.ZooKeeper" destroy-method="destroy">
                <property name="connectString" value="s-webapp-2.local.net:2181" />
            </bean>
        </property>
    </bean>

    <bean id="appConfig" class="com.smeet.loganalyzer.config.ApplicationConfig">
        <property name="environment" ref="production" />
    </bean>
</beans>
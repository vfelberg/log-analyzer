                    <property name="groupName" value="Jobs" />
                    <property name="hostConfigs">
                        <list>
                            <ref bean="blnrzh006" />
                        </list>
                    </property>
                    <property name="baseLogFileName" value="smeet-jobs.log" />
                </bean>
            </list>
        </property>
        <property name="zooKeeper">
            <bean class="com.smeet.zookeeper.ZooKeeper" destroy-method="destroy">
                <property name="connectString" value="blnrzh014.local.net:2181,blnrzh016.local.net:2181,blnrzh017.local.net:2181,blnrzh031.local.net:2181,blnrzh032.local.net:2181" />

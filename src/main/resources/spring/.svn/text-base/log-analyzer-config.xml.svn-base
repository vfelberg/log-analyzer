                    <property name="hostConfigs">
                        <list>
                            <ref bean="blnrzh016" />
                            <ref bean="blnrzh017" />
                        </list>
                    </property>
                    <property name="baseLogFileName" value="smeet-web.log" />
                </bean>
                <bean class="com.smeet.loganalyzer.config.HostGroupConfig">
                    <property name="groupName" value="Services" />
                    <property name="hostConfigs">
                        <list>
                            <ref bean="blnrzh022" />
                            <ref bean="blnrzh023" />
                            <ref bean="blnrzh034" />
                            <ref bean="blnrzh035" />
                            <ref bean="blnrzh036" />
                        </list>
                    </property>
                    <property name="baseLogFileName" value="smeet-services.log" />
                </bean>
                <bean class="com.smeet.loganalyzer.config.HostGroupConfig">
                    <property name="groupName" value="Room Services" />
                    <property name="hostConfigs">
                        <list>
                            <ref bean="blnrzh025" />
                            <ref bean="blnrzh026" />
                            <ref bean="blnrzh027" />
                            <ref bean="blnrzh028" />
                            <ref bean="blnrzh029" />
                            <ref bean="blnrzh030" />
                        </list>
                    </property>
                    <property name="baseLogFileName" value="smeet-services.log" />
                </bean>
                <bean class="com.smeet.loganalyzer.config.HostGroupConfig">
                    <property name="groupName" value="Video Screen" />
                    <property name="hostConfigs">
                        <list>
                            <ref bean="blnrzh025" />
                            <ref bean="blnrzh026" />
                            <ref bean="blnrzh027" />
                            <ref bean="blnrzh028" />
                            <ref bean="blnrzh029" />
                            <ref bean="blnrzh030" />
                        </list>
                    </property>
                    <property name="baseLogFileName" value="smeet-services-videoroom.log" />
                </bean>
                <bean class="com.smeet.loganalyzer.config.HostGroupConfig">
                    <property name="groupName" value="User Messaging" />
                    <property name="hostConfigs">
                        <list>
                            <ref bean="blnrzh022" />
                            <ref bean="blnrzh023" />
                            <ref bean="blnrzh034" />
                            <ref bean="blnrzh035" />
                            <ref bean="blnrzh036" />
                        </list>
                    </property>
                    <property name="baseLogFileName" value="smeet-rtmp.log" />
                </bean>
                <bean class="com.smeet.loganalyzer.config.HostGroupConfig">
                    <property name="groupName" value="OnlineState" />
                    <property name="hostConfigs">
                        <list>
                            <ref bean="blnrzh022" />
                            <ref bean="blnrzh023" />
                            <ref bean="blnrzh025" />
                            <ref bean="blnrzh026" />
                            <ref bean="blnrzh027" />
                            <ref bean="blnrzh028" />
                            <ref bean="blnrzh029" />
                            <ref bean="blnrzh030" />
                            <ref bean="blnrzh034" />
                            <ref bean="blnrzh035" />
